{
  "appId": "com.elborracho.app",
  "appName": "el-borracho",
  "webDir": "www",
  "bundledWebRuntime": false,
  "server": {
    "androidScheme": "https"
  },
  "plugins": {
    "SplashScreen": {
      "launchShowDuration": 2000,
      "backgroundColor": "#111111",
      "androidScaleType": "CENTER_CROP",
      "showSpinner": false
    },
    "LiveUpdates": {
      "appId": "com.elborracho.app",
      "channel": "production",
      "autoUpdateMethod": "background",
      "maxVersions": 2
    }
  }
}
